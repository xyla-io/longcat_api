    "express-session": "^1.15.6",
    "json-stable-stringify": "^1.0.1",
    "jsonschema": "^1.2.2",
    "knex": "^0.20.1",
    "lodash": "^4.17.15",
    "luxon": "^1.17.2",
    "mongoose": "^5.7.7",
