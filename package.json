{
  "name": "longcat_api",
  "version": "0.0.1",
  "description": "An API server for the 'Longcat' service.",
  "main": "index.js",
  "scripts": {
    "test": "nodemon ./node_modules/jasmine/bin/jasmine.js",
    "start": "rm -f crashlog/server.crashlog; nodemon index.js $npm_package_config_port $npm_package_config_host $npm_package_config_ssl $npm_package_config_site $2",
    "document": "./document.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyla-io/longcat_api.git"
  },
  "author": "Xyla",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xyla-io/longcat_api/issues"
  },
  "homepage": "https://github.com/xyla-io/longcat_api#readme",
  "config": {
    "port": 3200,
    "host": "localhost",
    "ssl": false,
    "site": "http://localhost:4200"
  },
  "apidoc": {
    "name": "longcat_api",
    "title": "Longcat API Docs"
  },
  "dependencies": {
    "acl": "^0.4.11",
